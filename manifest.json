{
  "name": "iMoney Converter",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://v6.exchangerate-api.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
